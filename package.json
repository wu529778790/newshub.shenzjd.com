{
  "name": "nuxt-app",
  "version": "0.0.2",
  "type": "module",
  "private": true,
  "homepage": "https://newshub.shenzjd.com",
  "author": "shenzjd",
  "scripts": {
    "dev": "nuxt dev",
    "dev:host": "nuxt dev --host 0.0.0.0",
    "build": "nuxt build",
    "build:analyze": "ANALYZE=true nuxt build",
    "build:quiet": "cross-env NODE_OPTIONS=--no-deprecation nuxt build",
    "build:production": "cross-env NODE_ENV=production nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "preview:production": "cross-env NODE_ENV=production nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "type-check": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf .nuxt .output node_modules/.vite",
    "clean:all": "rm -rf .nuxt .output node_modules pnpm-lock.yaml"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "cheerio": "^1.1.2",
    "consola": "^3.4.2",
    "daisyui": "^5.0.50",
    "dayjs": "^1.11.13",
    "defu": "^6.1.4",
    "fast-xml-parser": "^5.2.5",
    "html2canvas": "^1.4.1",
    "iconv-lite": "^0.7.0",
    "md5": "^2.3.0",
    "nuxt": "^4.0.3",
    "tailwindcss": "3.4.3",
    "vue": "^3.5.18",
    "vuedraggable": "^4.1.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/md5": "^2.3.5",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.0.0",
    "happy-dom": "^18.0.1",
    "ofetch": "^1.4.1",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
